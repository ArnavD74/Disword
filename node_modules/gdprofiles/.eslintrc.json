{ 
  "extends": [ 
    "airbnb-base" 
  ], 
  "parserOptions": { 
    "ecmaVersion": 2018 
  }, 
  "env": { 
    "node": true, 
    "es6": true 
  }, 
  "rules": {
    "linebreak-style": "off",
    "func-names": "off",
    "prefer-const": "off",
    "no-extra-boolean-cast": "off",
    "no-nested-ternary": "off"
  }
}